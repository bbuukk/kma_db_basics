<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="155.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vdb.dev.Controllers.addMenu.AddCatalogController">
    <children>
        <Pane layoutX="24.0" layoutY="10.0" prefHeight="160.0" prefWidth="552.0" style="-fx-background-color: #EBF5FB;">
            <children>
                <TextField fx:id="catalogNameField" alignment="TOP_LEFT" layoutX="32.0" layoutY="61.0" prefHeight="31.0" prefWidth="475.0" promptText="Catalog name">
                    <font>
                        <Font name="Lucida Sans Italic" size="15.0" />
                    </font>
                </TextField>
                <Label layoutX="182.0" layoutY="14.0" text="Add Catalog:">
                    <font>
                        <Font name="Lucida Sans Italic" size="24.0" />
                    </font>
                </Label>
            <Button fx:id="createCatalog" layoutX="322.0" layoutY="109.0" onMouseClicked="#createNewCatalog" prefHeight="19.0" prefWidth="196.0" style="-fx-background-color: #5DADE2;" text="Create catalog">
               <font>
                  <Font name="Lucida Sans Regular" size="16.0" />
               </font>
            </Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
