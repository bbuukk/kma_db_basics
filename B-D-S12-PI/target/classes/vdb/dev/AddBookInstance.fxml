<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="248.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vdb.dev.Controllers.AddNewInstanceBookController">
<children>
    <Pane layoutX="24.0" layoutY="21.0" prefHeight="208.0" prefWidth="552.0" style="-fx-background-color: #EBF5FB;">
        <children>
            <TextField fx:id="bookTitleField" alignment="TOP_LEFT" layoutX="32.0" layoutY="61.0" prefHeight="31.0" prefWidth="475.0" promptText="Book title">
                <font>
                    <Font name="Lucida Sans Italic" size="15.0" />
                </font>
            </TextField>
            <TextField fx:id="ShelfField" alignment="TOP_LEFT" layoutX="32.0" layoutY="102.0" prefHeight="31.0" prefWidth="475.0" promptText="Shelf">
                <font>
                    <Font name="Lucida Sans Italic" size="15.0" />
                </font>
            </TextField>
            <Label layoutX="126.0" layoutY="14.0" text="Add new instance of a book:">
                <font>
                    <Font name="Lucida Sans Italic" size="24.0" />
                </font>
            </Label>
            <TextField fx:id="amountField" alignment="TOP_LEFT" layoutX="32.0" layoutY="148.0" prefHeight="31.0" prefWidth="475.0" promptText="Amount">
                <font>
                    <Font name="Lucida Sans Italic" size="15.0" />
                </font>
            </TextField>

        </children>
    </Pane>
</children>
</AnchorPane>
